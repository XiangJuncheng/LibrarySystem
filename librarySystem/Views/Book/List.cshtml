@model IEnumerable<librarySytstem.Domain.Entities.Book>

@{
    ViewBag.Title = "Books";
}

<h2>List</h2>
    @foreach (var p in Model)
    {
        <div class="item" style="border-bottom:1px dashed silver;">
            <h3>@p.Title</h3>
            <p>价格：@p.Price.ToString("c") </p>
        </div>
    }
